<%= form_for @box, url: plan_boxes_path do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>

  <%= f.label "Month and Year" %><br>
  <%= f.date_select :month_year,  { :discard_day => true } %><br><br>

  <%= f.label "[Select items]"%><br>
  <%= f.collection_check_boxes :item_ids, Item.all, :id, :title %><br>

  <%= f.submit %>

<% end %>
