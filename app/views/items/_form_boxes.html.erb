<%= form_for [@box, @item] do |f| %>
  <%= render 'shared/errors', object: @item %>

  <%= f.label :item_name %><br>
  <%= f.text_field :item_name %><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br>

  <%= f.label :image %><br>
  <%= f.text_field :image %><br>

  <%= f.label :size %><br>
  <%= f.text_field :size %><br>

  <%= f.label :url %><br>
  <%= f.text_field :url %><br><br>

  <%= f.submit %>
<% end %>
